{
  "headers": [
    {
      "source": "./src/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=86400"
        }
      ]
    },
    {
      "source": "/static/media/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=86400"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "https://grsl.vercel.app/sitemap.xml",
      "destination": "https://grsl.vercel.app/sitemap-app.xml"
    },
    {
      "source": "https://gsl.ramidev.tech/sitemap.xml",
      "destination": "https://gsl.ramidev.tech/sitemap-tech.xml"
    }
  ]
}
