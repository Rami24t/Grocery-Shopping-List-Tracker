{
  "headers": [
    {
      "source": "./src/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=86400"
        }
      ]
    },
    {
      "source": "/static/media/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=86400"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "has": [
        {
          "type": "host",
          "value": "grsl.vercel.app"
        }
      ],
      "destination": "/sitemap-app.xml"
    },
    {
      "source": "/sitemap.xml",
      "has": [
        {
          "type": "host",
          "value": "gsl.ramidev.tech"
        }
      ],
      "destination": "/sitemap-tech.xml"
    }
  ]
}